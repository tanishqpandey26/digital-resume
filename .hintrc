{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "width: fit-content",
          "scroll-behavior",
          "text-underline-offset",
          "min-height: fit-content",
          "height: fit-content",
          "scroll-behavior: smooth"
        ]
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 127",
    "not firefox <= 126",
    "not ios_saf <= 12.5",
    "not opera <= 109"
  ]
}